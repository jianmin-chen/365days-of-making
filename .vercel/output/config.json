{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_image.astro"
		},
		{
			"src": "^\\/$",
			"dest": "index.astro"
		},
		{
			"src": "^\\/dashboard\\/?$",
			"dest": "dashboard.astro"
		},
		{
			"src": "^\\/feed\\.xml$",
			"dest": "feed.xml.js"
		},
		{
			"src": "^\\/signin\\/?$",
			"dest": "signin.astro"
		},
		{
			"src": "^\\/entry\\/([^/]+?)\\/?$",
			"dest": "entry/[slug].astro"
		},
		{
			"src": "^\\/start\\/?$",
			"dest": "start.astro"
		},
		{
			"src": "^\\/api\\/subscribe$",
			"dest": "api/subscribe.ts"
		},
		{
			"src": "^\\/api\\/webring$",
			"dest": "api/webring.ts"
		},
		{
			"src": "^\\/api\\/auth\\/signout$",
			"dest": "api/auth/signout.ts"
		},
		{
			"src": "^\\/api\\/auth\\/signin$",
			"dest": "api/auth/signin.ts"
		},
		{
			"src": "^\\/api\\/send$",
			"dest": "api/send.ts"
		}
	]
}